[program:dnsmasq]
command={{ homebrew_install_path }}/opt/dnsmasq/sbin/dnsmasq --keep-in-foreground -C {{ homebrew_install_path }}/etc/dnsmasq.conf
directory={{ homebrew_install_path }}/opt/dnsmasq
stdout_logfile={{ homebrew_install_path }}/var/log/dnsmasq.log
stderr_logfile={{ homebrew_install_path }}/var/log/dnsmasq.err
autorestart=true

