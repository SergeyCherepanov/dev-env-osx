[PHP]
short_open_tag=off
detect_unicode=off
max_execution_time = 900
memory_limit = 4G
upload_max_filesize = 256M
post_max_size = 256M
display_errors=on
error_reporting=E_ALL
;error_reporting=E_ALL ^ E_DEPRECATED
max_input_vars = 100000
mysqli.default_socket = /usr/local/var/run/mysqld/5.6/mysqld.sock
pdo_mysql.default_socket = /usr/local/var/run/mysqld/5.6/mysqld.sock
sendmail_path = /usr/local/bin/fake_sendmail.sh
 
[Date]
date.timezone=UTC
 
[xdebug]
zend_extension="/usr/local/opt/php71-xdebug/xdebug.so"
xdebug.remote_enable=1
xdebug.remote_autostart = 0
xdebug.remote_connect_back = 0
xdebug.remote_host=127.0.0.1
xdebug.remote_port=9271
xdebug.profiler_enable=0
xdebug.profiler_output_dir=/tmp/xdebug71
xdebug.idekey=PHPSTORM
xdebug.overload_var_dump=0
xdebug.max_nesting_level=500
