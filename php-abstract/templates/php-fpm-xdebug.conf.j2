[global]
daemonize = no
[www]
user = {{ brew_user }}
group = {{ brew_group }}
listen = 127.0.0.1:91{{ php_version_num_int }}
listen.mode = 0666
pm = dynamic
pm.max_children = 10
pm.start_servers = 3
pm.min_spare_servers = 2
pm.max_spare_servers = 5
catch_workers_output = yes
env[PHP_PATH] = {{ brew_install_path }}/opt/{{ php_version }}/bin/php
